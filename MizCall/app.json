{
  "expo": {
    "name": "MizCall",
    "slug": "MizCall",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mizcall",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "notification": {
      "icon": "./assets/images/icon.png",
      "color": "#3c82f6",
      "androidMode": "default",
      "androidCollapsedTitle": "MizCall"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mizcall.app",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "MizCall needs microphone access for audio calls.",
        "NSLocalNetworkUsageDescription": "Allow this app to discover development servers on your local network."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.mizcall.app",
      "permissions": [
        "RECORD_AUDIO",
        "WAKE_LOCK",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.POST_NOTIFICATIONS"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0aa170c5-60bd-47c2-8015-cebd6a3717b6"
      }
    }
  }
}
